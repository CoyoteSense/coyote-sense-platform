{
  "name": "@coyotesense/oauth2-client-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for OAuth2 authentication client libraries",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:typescript": "jest --testPathPattern=typescript",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf coverage dist node_modules/.cache"
  },
  "keywords": [
    "oauth2",
    "authentication",
    "security",
    "coyotesense",
    "testing"
  ],
  "author": "CoyoteSense Platform Team",
  "license": "MIT",
  "overrides": {
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "node-fetch": "^3.3.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
